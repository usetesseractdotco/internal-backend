{
  "name": "@tesseract-ai/internal-backend",
  "version": "0.1.0",
  "module": "src/main.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --env-file .env --watch src/main.ts",
    "build": "bun build src/main.ts",
    "start": "NODE_ENV=prod bun src/main.ts",
    "test": "bun --env-file .env vitest",
    "lint": "bun eslint --fix src --ext ts",
    "db:generate": "drizzle-kit generate --name",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bun": "latest",
    "@types/pg": "^8.15.1",
    "drizzle-kit": "^0.31.1",
    "eslint": "8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "unplugin-swc": "^1.5.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcryptjs": "^3.0.2",
    "drizzle-orm": "^0.43.1",
    "fastify": "^5.3.2",
    "fastify-type-provider-zod": "^4.0.2",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "pg": "^8.15.6",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.4"
  }
